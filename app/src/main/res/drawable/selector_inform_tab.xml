<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--选中时的图片背景-->
    <item android:drawable="@color/bg_7dd9fd" android:state_selected="true" />
    <item android:drawable="@color/bg_7dd9fd" android:state_pressed="true" />
    <!-- 默认时的背景图片-->
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <!-- 填充 -->
            <solid android:color="@color/transparent" /><!-- 填充的颜色 -->

            <!-- 描边 -->
            <stroke android:width="0.5dp" android:color="@color/bg_7dd9fd" />
        </shape>
    </item>

    <!-- 默认时的背景图片-->
    <item android:drawable="@color/bg_275681" />
</selector>