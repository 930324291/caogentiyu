<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"

    android:useLevel="false">

    <!-- 圆角 -->
    <corners
        android:bottomLeftRadius="12dp"/><!-- 设置圆角半径 -->

    <!-- 填充 -->
    <solid
        android:color="#182a39"/><!-- 填充的颜色 -->

    <!-- 描边 -->
